{
  "manifest_version": 3,
  "name": "Tabs to EPUB",
  "version": "0.1.0",
  "description": "Save one or more open tabs as a Kindle-friendly EPUB.",
  "key": "",
  "permissions": ["contextMenus", "scripting", "storage", "downloads", "tabs", "identity"],
  "oauth2": {
    "client_id": "31183752331-dv8sgdo3hc9k8mchoo7ofaoebi7jhsu9.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/gmail.send"]
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "extension/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Tabs to EPUB",
    "default_popup": "extension/popup.html"
  }
}
