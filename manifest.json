{
  "manifest_version": 3,
  "name": "Tabs to EPUB & Kindle",
  "version": "0.6.0",
  "description": "Save one or more tabs as Kindle-friendly outputs: article tabs to EPUB, PDF tabs as separate PDFs, with optional Kindle email delivery.",
  "key": "",
  "icons": {
    "16": "extension/icons/icon-16.png",
    "48": "extension/icons/icon-48.png",
    "128": "extension/icons/icon-128.png"
  },
  "permissions": ["contextMenus", "scripting", "storage", "downloads", "tabs", "identity"],
  "oauth2": {
    "client_id": "31183752331-dv8sgdo3hc9k8mchoo7ofaoebi7jhsu9.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/gmail.send"]
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "extension/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Tabs to EPUB & Kindle",
    "default_icon": {
      "16": "extension/icons/icon-16.png",
      "48": "extension/icons/icon-48.png"
    },
    "default_popup": "extension/popup.html"
  }
}
